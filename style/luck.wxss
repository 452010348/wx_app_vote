/**
组合用法1.  d-flex flex-center-x flex-center-y 
组合用法2.  d-flex flex-center-x flex-center-y flex-around  || d-flex flex-around 
组合用法3.  d-flex flex-center-x flex-center-y flex-between || d-flex flex-between
组合用法6.  d-flex d-flex-column    //垂直
用法很多...
*/
/*水平布局*/
.d-flex {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
}

.flex-wrap {
  -ms-flex-wrap: wrap;
      flex-wrap: wrap;
}
/*垂直布局*/
.d-flex-column {
 /*  -webkit-box-orient: vertical;
  -webkit-box-dirpxtion: normal; */
      /* -ms-flex-dirpxtion: column; */
  flex-direction: column;
}
.flex-around {
  /* -ms-flex-pack   : distribute; */
  justify-content : space-around;
}
.flex-between {
  /* -webkit-box-pack: justify; */
  /* -ms-flex-pack   : justify; */
  justify-content : space-between;
}

.flex-center-x{
  justify-content: center;
}
.flex-center-y{
  align-items: center;
  align-self: center;
}

.flex-top{
  align-items: flex-start;
  align-self: flex-start;
}
.flex-bottom{
  align-items: flex-end;
  align-self: flex-end;
}
.flex-left{
  /* -webkit-box-pack: start;
  -ms-flex-pack   : start; */
  justify-content: flex-start;
}
.flex-right{
  /* -webkit-box-pack: end;
  -ms-flex-pack   : end; */
  justify-content: flex-end;
}

.flex-1 {
  -webkit-box-flex: 1;
      -ms-flex: 1;
          flex: 1;
}
.flex-2 {
  -webkit-box-flex: 2;
      -ms-flex: 2;
          flex: 2;
}
.flex-grow-1 {
  -webkit-box-flex: 1;
      -ms-flex-positive: 1;
          flex-grow: 1;
}
.flex-grow-2 {
  -webkit-box-flex: 2;
      -ms-flex-positive: 2;
          flex-grow: 2;
}
/*----------------------*/


/*子盒子独立*/
.flex-self-bottom,.flex-self-right{align-self:flex-end;}
.flex-self-top,.flex-self-left{align-self:flex-start;}
.flex-self-center{align-self:center;}
/*----------------------*/


/*文本不换行 超出隐藏 省略..*/
.text-ellipsis-1,.css-ellipsis{white-space:nowrap;text-overflow:ellipsis;-o-text-overflow:ellipsis;overflow:hidden;}
/*超过两行省略*/
.text-ellipsis-2{
  display: -webkit-box; /*设置display，将对象作为弹性伸缩盒子模型显示*/
  -webkit-line-clamp: 2;   /*限制在一个块元素显示的文本的行数*/
  -webkit-box-orient: vertical; /*规定框的子元素应该被水平或垂直排列*/
  overflow: hidden;
  text-overflow: ellipsis;
}

